{
  "manifest_version": 2,
  "name": "Cloudy HN",
  "version": "0.1.0",
  "description": "Changes the size of links on Hacker News based on their score.",
  "icons": { 
    "128": "icon_128.png" 
  },
  "content_scripts": [
    {
      "matches": ["*://news.ycombinator.com/*"],
      "js" : ["cloudyhn.js"]
    }
  ]
}
